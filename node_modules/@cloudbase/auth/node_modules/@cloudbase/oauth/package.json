{
	"name": "@cloudbase/oauth",
	"version": "2.5.6-beta.1",
	"description": "cloudbase javascript sdk auth componets",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
