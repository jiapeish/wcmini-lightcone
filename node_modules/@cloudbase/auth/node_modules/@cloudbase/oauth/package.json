		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-prettier": "^3.1.2",
		"lint-staged": "^13.0.3",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.0",
		"typescript": "^3.8.3"
	},
	"lint-staged": {
		"**/*.ts": [
			"eslint --fix"
