{
  "name": "@cloudbase/weda-cloud-sdk",
  "version": "1.0.93",
  "main": "dist/h5/index.js",
  "module": "esm/h5/index.js",
  "miniprogram": "dist/miniprogram",
  "files": [
    "dist/**/*",
    "esm/**/*",
    "scripts/*"
  ],
  "scripts": {
    "clean": "rimraf dist esm",
    "prebuild": "yarn clean",
    "build": "tsc && tsc -p tsconfig.json -m es2020 --outDir esm && node ./scripts/version",
    "build:r": "rollup -c rollup.config.ts",
    "prepublish": "yarn build; yarn build:r",
    "dev": "tsc --watch src"
  },
  "dependencies": {
    "@cloudbase/adapter-interface": "^0.5.0",
    "@isaacs/ttlcache": "^1.4.1"
  },
  "devDependencies": {
    "@cloudbase/auth": "^2.8.23-beta.0",
    "@cloudbase/js-sdk": "^2.8.23-beta.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-typescript": "^8.3.1",
    "mobx": "^5",
    "rollup": "^2.36.2",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4"
  },
  "peerDependencies": {
    "@cloudbase/js-sdk": "*",
    "mobx": "^5"
  },
  "@cloudbase/js-sdk": {
    "optional": true
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}