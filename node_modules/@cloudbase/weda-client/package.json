{
  "name": "@cloudbase/weda-client",
  "packageManager": "yarn@3.1.0",
  "version": "1.1.14",
  "main": "dist/cjs/web/index.js",
  "module": "dist/esm/web/index.js",
  "miniprogram": "miniprogram",
  "typedocMain": "src/types/index.ts",
  "scripts": {
    "develop": "tsc --watch",
    "develop:esm": "tsc -p tsconfig.json -m ESNext --outDir dist/esm --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist miniprogram",
    "build": "yarn clean && yarn build:lib && yarn build:miniprogram",
    "build:lib": "tsc && tsc -p tsconfig.json -m ESNext --outDir dist/esm",
    "build:miniprogram": "tsc -p tsconfig.mp.json && cp -rf ./src/mp/runtime/placeholder ./miniprogram/mp/runtime/placeholder && rollup -c rollup.config.ts",
    "clean:workspace": "rm -rf node_modules yarn.lock package-lock.json",
    "docs": "typedoc --options typedoc.json"
  },
  "dependencies": {
    "dayjs": "^1.11.5",
    "miniprogram-api-promise": "^1.0.4",
    "mobx": "^5.15.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-env": "^7.16.5",
    "@cloudbase/weda-cloud-sdk": "^1.0.89",
    "@rollup/plugin-commonjs": "~21.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/jest": "^27.5.1",
    "babel-jest": "^27.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-location-mock": "^1.0.9",
    "lodash-es": "^4.17.19",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "ts-jest": "^27.1.5",
    "typedoc": "^0.22.17",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "@cloudbase/weda-cloud-sdk": "^1.0.89",
    "@tcwd/weapps-sdk": "1.2.13-rc.2",
    "@zxing/library": "^0.18.6",
    "decimal.js": "^10.4.3",
    "papaparse": "^5.4.1",
    "react": ">=16.12.0",
    "react-dom": ">=16.8.6",
    "xlsx": "^0.18.5"
  },
  "peerDependenciesMeta": {
    "@tcwd/weapps-sdk": {
      "optional": true
    }
  }
}